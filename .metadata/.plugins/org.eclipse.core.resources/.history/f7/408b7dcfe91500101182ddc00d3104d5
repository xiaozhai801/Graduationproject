package com.zpq.pojo;

import lombok.Data;

@Data
public class User {
	private int userId;
	private String name;
	private String password;
	private String sex;
	private int age;
	private String email;
	
	public void setUserId(int userId) {
		this.userId=userId;
	}
	public int getUserId() {
		return userId;
	}
	
}
